{
  "name": "coffee-factory-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:file": "cross-env eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint": "cross-env eslint . --ext .js,.jsx,.ts,.tsx --fix.",
    "pretty": "cross-env fixme=fixme prettier --write '**/*.{j,t}{s,sx}'",
    "pretty:watch": "cross-env fixme=fixme onchange '**/*.{j,t}{s,sx}' -- prettier --write {{changed}}",
    "ts-check": "cross-env tsc",
    "ts-check:watch": "yarn ts-check -- --watch"
  },
  "dependencies": {
    "@types/postcss-preset-env": "^6.7.2",
    "@types/tailwindcss": "^2.0.3",
    "antd": "^4.15.6",
    "antd-mobile": "^2.3.4",
    "next": "10.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "sass": "^1.32.13",
    "styled-components": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@types/styled-components": "^5.1.9",
    "autoprefixer": "^10.2.5",
    "babel-plugin-styled-components": "^1.12.0",
    "postcss": "^8.2.13",
    "typescript": "^4.2.4"
  }
}
